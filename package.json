{
  "name": "ubirch-web-ui",
  "version": "0.9.0",
  "description": "Web frontend for managing your ubirch devices",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubirch/ubirch-web-ui.git"
  },
  "author": "ubirch GmbH - Beate Fiss",
  "homepage": "https://github.com/ubirch/ubirch-web-ui",
  "scripts": {
    "ng": "ng",
    "start": "./run-local.sh",
    "prod": "ng serve --host 0.0.0.0 --port 80 --disable-host-check",
    "prodtest": "ng serve --host 0.0.0.0 --port 80 --disable-host-check",
    "local": "ng serve --configuration=local",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "supportedBrowsers": "(echo module.exports = && browserslist-useragent-regexp --allowHigherVersions) > src/supportedBrowsers.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.3",
    "@angular/cdk": "^15.2.3",
    "@angular/common": "^15.2.3",
