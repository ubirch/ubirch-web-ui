    "@angular/cli": "^15.2.4",
    "@angular/compiler": "^15.2.3",
    "@angular/compiler-cli": "^15.2.3",
    "@angular/language-service": "^15.2.3",
    "@ionic/angular-toolkit": "~6.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^6.0.1",
    "cordova-plugin-device": "^2.0.2",
